<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="sale_order_history_wizard_form_view" model="ir.ui.view">
		<field name="name">sale.order.history.wizard.form.view</field>
		<field name="model">sale.order.history.wizard</field>
		<field name="arch" type="xml">
			<form create="0" edit="0">		
				<group>
					<group>
						<field name="product_id" nolabel="1" readonly="1"/>
					</group>
				</group>			
				<group>
					<field name="order_line_ids" nolabel="1" readonly="1">
						<tree>
							<field name="order_id"/>
							<field name="price_unit"/>
							<field name="product_uom_qty"/>
							<field name="price_subtotal"/>
						</tree>
					</field>
				</group>
				<footer>
					<button name="action_cancel" special="cancel" string="Cancel"/>
				</footer>
			</form>
		</field>
	</record>
</odoo>